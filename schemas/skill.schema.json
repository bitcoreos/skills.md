{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": { "type": "string" },
    "spdx-id": { "type": "string" },
    "origin": { "type": "string", "format": "uri" },
    "hash": { "type": "string", "pattern": "^sha256:[a-f0-9]{64}$" },
    "version": { "type": "string" }
  },
  "required": ["id", "spdx-id", "hash"]
}
